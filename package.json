{
    "name": "woonuxt",
    "private": true,
    "scripts": {
        "build": "nuxt build",
        "dev": "nuxt dev --host",
        "dev:ssl": "NODE_TLS_REJECT_UNAUTHORIZED=0 nuxt dev --https --ssl-cert localhost.pem --ssl-key localhost-key.pem",
        "generate": "nuxt generate",
        "preview": "nuxt preview",
        "postinstall": "nuxt prepare",
        "serve": "nuxi generate && npx serve .output/public -l 8080",
        "deploy": "gh-pages -d dist"
    },
    "dependencies": {
        "@stripe/stripe-js": "^4.10.0",
        "@vueform/slider": "^2.1.10",
        "@vueuse/core": "^12.4.0"
    },
    "devDependencies": {
        "@nuxt/icon": "^1.10.3",
        "@nuxt/image": "1.9.0",
        "@nuxtjs/i18n": "^8.5.5",
        "@nuxtjs/tailwindcss": "^6.13.1",
        "@tailwindcss/typography": "^0.5.16",
        "gh-pages": "^6.3.0",
        "nuxt": "^3.15.2",
        "nuxt-graphql-client": "^0.2.43",
        "prettier": "^3.4.2",
        "woonuxt-settings": "^1.0.61"
    },
    "overrides": {
        "sharp": "0.32.6"
    }
}
